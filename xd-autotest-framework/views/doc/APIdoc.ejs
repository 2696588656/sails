<script>
   $('#save_doc').click(function(){
     var inputParam={requestName:'newLogin'};
     var queryString="newLogin";
     console.log(" test ... :queryString="+queryString);

     $.ajax({
       url: '/doc/findRequestItemByName',
       type:"Get",
       //data:"{'first':'蚂蚁部落','second':'欢迎您'}",
       //data:queryString,
       data:{
         requestName: queryString,
         //b: $('#b').val()
       },
       success:function(data){
         //返回的数据用data.d获取内容
         //alert(data.d);
         $("textarea#textarea_response").val(JSON.stringify(data));
       },
       error:function(err){
         //alert(err);
         $("textarea#textarea_response").val(JSON.stringify(err));
       }
     });
     console.log("execute done service via ajax... this should be seen on front...");
   });
 </script>

